    "extra": {
        "laravel": {
            "providers": [
                "Amin3536\\PassportOtpGrant\\PassportOtpGrantServiceProvider"
            ],
            "aliases": {
                "PassportOtpGrant": "Amin3536\\PassportOtpGrant\\Facades\\PassportOtpGrant"
            }
        }
    }
